{
  "name": "wayfare",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel ./server -d ./build",
    "clean": "rm -rf ./build && mkdir build",
    "devStart": "set DEBUG=http && nodemon --exec babel-node ./server/index.js",
    "start": " node ./build/index.js",
    "test": "cross-env NODE_ENV=test && npm run resetTables && nyc --require @babel/register --require @babel/polyfill mocha  ./server/test/*.js --timeout 20000 --exit",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov-info",
    "coveralls-coverage": "coveralls < lcov-info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov-info",
    "coverage": "npm run test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
    "lint": "./node_modules/.bin/eslint index.js",
    "dropTables": "babel-node ./server/models/tables dropTables",
    "createTables": "babel-node ./server/models/tables createTables",
    "resetTables": "npm run dropTables && npm run createTables"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.5.4",
    "@hapi/joi": "^15.1.0",
    "bcrypt": "^3.0.6",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "nyc": "^14.1.1",
    "pg": "^7.11.0",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^10.2.0",
    "nodemon": "^1.19.1"
  },
  "engines": {
    "node": "10.16.0",
    "npm": "6.10.0"
  }
}
